<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Pipeline" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WorkerRole name="ScoringWorker" vmsize="Small">
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="DB_SERVER" />
      <Setting name="DB_NAME" />
      <Setting name="DB_PASSWORD" />
      <Setting name="DB_USER" />
      <Setting name="STORAGE_ACCOUNT" />
      <Setting name="STORAGE_KEY" />
      <Setting name="QUEUE_SCORING" />
      <Setting name="QUEUE_NEW_IDS" />
      <Setting name="LOG_LEVEL" />
      <Setting name="LOG_STORAGE_ACCOUNT" />
      <Setting name="LOG_STORAGE_KEY" />
    </ConfigurationSettings>
    <Imports>
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <Startup>
      <Task commandLine="setup_worker.cmd &gt; log.txt" executionContext="elevated" taskType="simple">
        <Environment>
          <Variable name="EMULATED">
            <RoleInstanceValue xpath="/RoleEnvironment/Deployment/@emulated" />
          </Variable>
          <Variable name="RUNTIMEID" value="node" />
          <Variable name="RUNTIMEURL" value="http://az413943.vo.msecnd.net/node/0.10.21.exe;http://nodertncu.blob.core.windows.net/iisnode/0.1.21.exe" />
        </Environment>
      </Task>
      <Task commandLine="node.cmd .\startup.js" executionContext="elevated" />
    </Startup>
    <Runtime>
      <Environment>
        <Variable name="EMULATED">
          <RoleInstanceValue xpath="/RoleEnvironment/Deployment/@emulated" />
        </Variable>
        <Variable name="DB_SERVER">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='DB_SERVER']/@value" />
        </Variable>
        <Variable name="DB_NAME">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='DB_NAME']/@value" />
        </Variable>
        <Variable name="DB_PASSWORD">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='DB_PASSWORD']/@value" />
        </Variable>
        <Variable name="DB_USER">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='DB_USER']/@value" />
        </Variable>
        <Variable name="STORAGE_ACCOUNT">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='STORAGE_ACCOUNT']/@value" />
        </Variable>
        <Variable name="STORAGE_KEY">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='STORAGE_KEY']/@value" />
        </Variable>
        <Variable name="QUEUE_SCORING">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='QUEUE_SCORING']/@value" />
        </Variable>
        <Variable name="QUEUE_NEW_IDS">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='QUEUE_NEW_IDS']/@value" />
        </Variable>
        <Variable name="LOG_LEVEL">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='LOG_LEVEL']/@value" />
        </Variable>
        <Variable name="LOG_STORAGE_ACCOUNT">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='LOG_STORAGE_ACCOUNT']/@value" />
        </Variable>
        <Variable name="LOG_STORAGE_KEY">
          <RoleInstanceValue xpath="/RoleEnvironment/CurrentInstance/ConfigurationSettings/ConfigurationSetting[@name='LOG_STORAGE_KEY']/@value" />
        </Variable>
      </Environment>
      <EntryPoint>
        <ProgramEntryPoint commandLine="node.cmd .\worker.js" setReadyOnProcessStart="true" />
      </EntryPoint>
    </Runtime>
  </WorkerRole>
